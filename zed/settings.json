{
  "edit_predictions": {
    "provider": "zed",
  },
  "soft_wrap": "bounded",
  "agent": {
    "always_allow_tool_actions": true,
    "default_profile": "ask",
    "inline_assistant_model": {
      "provider": "openai",
      "model": "gpt-5-mini",
    },
    "default_model": {
      "provider": "openai",
      "model": "gpt-5-mini",
    },
    "model_parameters": [],
  },
  "languages": {
    "JavaScript": {
      "formatter": {
        "external": {
          "command": "prettier",
          "arguments": ["--stdin-filepath", "{buffer_path}"],
        },
      },
      "code_actions_on_format": {
        "source.fixAll.eslint": true,
      },
    },
  },
  "lsp": {
    "vscode-css-language-server": {
      "settings": {
        "css": {
          "lint": {
            "unknownAtRules": "ignore",
          },
        },
      },
    },
  },
}
